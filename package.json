{
  "name": "gaze-anchor",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "license": "MIT",
  "author": {
    "name": "Ethan Teng"
  },
  "keywords": [],
  "scripts": {
    "test": "jest",
    "test:ncc": "jest --no-cache",
    "test:cover": "jest --coverage",
    "test:verbose": "jest --verbose",
    "build": "tsc && vite build",
    "format": "prettier --write \"packages/**/*.ts?(x)\" && eslint --fix --ext .ts packages",
    "format:package": "prettier --write \"packages/**/package.json\"",
    "merge-and-push": "node ./scripts/git.js"
  },
  "main": "./dist/gaze-anchor.umd.js",
  "module": "./dist/gaze-anchor.es.js",
  "exports": {
    ".": {
      "import": "./dist/gaze-anchor.es.js",
      "require": "./dist/gaze-anchor.umd.js"
    }
  },
  "typings": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-plugin-html": "^7.1.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.27.2"
  }
}
